[32m+[m[32m            }[m
[32m+[m[32m            else if(num[i][j-1]>num[i-1][j])[m
[32m+[m[32m            {[m
[32m+[m[32m                num[i][j]=num[i][j-1];[m
[32m+[m[32m                flag[i][j]=2;  ///ÏòÓÒ±ê¼Ç[m
[32m+[m[32m            }[m
[32m+[m[32m            else[m
[32m+[m[32m            {[m
[32m+[m[32m                num[i][j]=num[i-1][j];[m
[32m+[m[32m                flag[i][j]=3;  ///ÏòÏÂ±ê¼Ç[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
